<label class="col-sm-6 control-label">{{displayName}}</label>
<div class="col-sm-6">
  <input value=""
    data-type="{{ type }}"
    data-provide="slider"
    data-slider-min="{{ min }}"
    data-slider-max="{{ max }}"
    data-slider-step="1"
    data-slider-value="[{{ min }},{{ max }}]"
    data-slider-tooltip=""
    />
</div>
